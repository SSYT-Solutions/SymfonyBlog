{
    "name": "slim/slim-blog",
    "description": "A Slim Framework blog application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/SSYT-Solutions/SymfonyBlog",
    "license": "MIT",

    "require": {
        "php": "^5.5 || ^7.0",
        "ext-mbstring": "*",
        "andrewdyer/slim3-session-middleware": "^0.1.0",
        "monolog/monolog": "^1.17",
        "phpmailer/phpmailer": "^6.1",
        "psr/event-dispatcher": "^1.0",
        "robmorgan/phinx": "^0.5.1",
        "slim/csrf": "^0.8",
        "slim/flash": "^0.4",
        "slim/slim": "3.12",
        "slim/twig-view": "^2.5",
        "symfony/event-dispatcher": "^4.3",
        "twig/twig": "^2.8"
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },

    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
